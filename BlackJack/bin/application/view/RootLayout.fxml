<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.RootController">
   <children>
      <MenuBar>
        <menus>
          <Menu mnemonicParsing="false" text="Games">
            <items>
                  <CheckMenuItem fx:id="blackjack" mnemonicParsing="false" onAction="#handelBlackjack" selected="true" text="Blackjack" />
                  <CheckMenuItem fx:id="poker" mnemonicParsing="false" onAction="#handelPoker" text="Poker" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </children>
</VBox>
